{
  "name": "nete",
  "productName": "Nete",
  "version": "0.1.1",
  "description": "Pete electron",
  "main": "./dist/main.bundle.js",
  "build": {
    "appId": "com.mayreh.Nete",
    "mac": {
      "target": ["dmg"],
      "extendInfo": {
        "LSUIElement": 1
      }
    },
    "directories": {
      "buildResources": "assets",
      "output": "build"
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "webpack --mode=development --watch --progress --colors",
    "prod": "webpack --mode=production --progress --colors",
    "pack": "yarn prod && electron-builder --dir",
    "dist": "yarn prod && electron-builder"
  },
  "author": "ocadaruma",
  "license": "Apache-2.0",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "buefy": "^0.8.12",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "electron": "^11.5.0",
    "electron-builder": "^22.3.2",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "vue-class-component": "^7.2.3",
    "vue-loader": "^15.9.0",
    "vue-property-decorator": "^8.4.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
